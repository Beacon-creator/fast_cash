<?xml version="1.0" encoding="UTF-8"?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Fast_Cash.Styles.ColorStyles">

    <!-- back Colors -->
    <Color x:Key="PrimaryBackgroundColor" >#FF6E6A</Color>
    <Color x:Key="PrimaryBackgroundColor2" >Transparent</Color>
    <Color x:Key="SecundaryBackgroundColor" >#FFFFFF</Color>
    <Color x:Key="SecundaryBackgroundColor2" >#496FFF</Color>
    <Color x:Key="TertiaryBackgroundColor" >#C3CBCE</Color>

    <!-- Primary Colors -->
    <Color x:Key="PrimaryBlueColor" >#496FFF</Color>
    <Color x:Key="PrimaryGrayColor" >#496FFF</Color>

    <!-- Button Colors -->
    <Color x:Key="ProButtonColor" >#CCCCCC</Color>
    <Color x:Key="ButtonbackColor" >#ff6e6a</Color>

    
    <!-- Text Colors -->
    <Color x:Key="PrimaryTextColor">#0D0D0D</Color>
    <Color x:Key="PrimaryTextColor2">#1A181B</Color>
    <Color x:Key="SecundaryTextColor">#707070</Color>
    <Color x:Key="SecundaryTextColor2">#070707</Color>
    <Color x:Key="TertiaryTextColor">#FE8D7B</Color>
    <Color x:Key="TertiaryTextColor2">#949494</Color>
    <Color x:Key="ProTextColor" >#ffffff</Color>
    <Color x:Key="ProTextColor2" >#000000</Color>
</ResourceDictionary>