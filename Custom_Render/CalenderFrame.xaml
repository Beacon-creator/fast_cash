<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Fast_Cash.Custom_Render.CalenderFrame">
    <Frame CornerRadius="10" HasShadow="True" BorderColor="{DynamicResource ProTextColor}">
        <HorizontalStackLayout>
            <ImageButton Source="calender.svg" Clicked="OnCalendarButtonClicked"/>
            <Label x:Name="SelectedDateLabel" Text="Select a date" VerticalOptions="Center"/>
        </HorizontalStackLayout>
    </Frame>
</ContentView>
