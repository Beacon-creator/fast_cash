<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Cashnal.Custom_Render.TopNav">
    <HorizontalStackLayout Spacing="15" Padding="0,20">
        <ImageButton WidthRequest="20" HeightRequest="20" Clicked="BackButton_Clicked" Source="icn_back.svg"/>
        <Label FontSize="Medium" TextColor="{DynamicResource PrimaryTextColor}" Text="{Binding Header}" />
    </HorizontalStackLayout>
</ContentView>
